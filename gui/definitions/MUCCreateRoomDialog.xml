<interface>
  <object class="GtkWindow" id="createRoomWindow">
    <property name="width_request">580</property>
    <property name="title" translatable="yes">Create new chat room</property>
    <property name="resizable">False</property>
    <signal name="destroy" handler="on_close_window" swapped="no"/>
    <child>
      <object class="GtkBox" id="content">
        <property name="hexpand">True</property>
        <property name="vexpand">True</property>
        <property name="orientation">vertical</property>
      </object>
    </child>
    <style>
      <class name="muc"/>
    </style>
  </object>
</interface>
